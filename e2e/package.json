{
  "name": "polis-e2e",
  "version": "1.0.0",
  "description": "End-to-End test suite for polis, using Cypress.js",
  "main": "index.js",
  "scripts": {
    "start": "cypress open",
    "test": "cypress run --spec 'cypress/e2e/!(third-party)/**/*.cy.js'",
    "test:all": "cypress run --spec 'cypress/**/*.cy.js'",
    "e2e": "npm test",
    "e2e:chrome": "npm test -- --browser chrome",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build:embed": "node build-embed.js",
    "build:integrated": "node build-integrated.js",
    "seed": "node scripts/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "cypress": "^14.2.0",
    "cypress-terminal-report": "^7.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-mocha": "^10.5.0",
    "prettier": "^3.5.3"
  }
}
